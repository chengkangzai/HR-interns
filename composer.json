{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "barryvdh/laravel-dompdf": "^2.2.0",
        "chengkangzai/laravel-office-holidays": "^0.0.1",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.0",
        "guzzlehttp/guzzle": "^7.10.0",
        "laravel/framework": "^11.45.2",
        "laravel/sanctum": "^4.2.0",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29",
        "saade/filament-fullcalendar": "^4.0",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/pdf-to-text": "^1.54.1",
        "ysfkaya/filament-phone-input": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "filament/upgrade": "^4.0",
        "laravel/boost": "^1.6",
        "laravel/pint": "^1.24.0",
        "laravel/sail": "^1.45.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^4.1",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
