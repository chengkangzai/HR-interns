{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.2.0",
        "chengkangzai/laravel-office-holidays": "^0.0.1",
        "croustibat/filament-jobs-monitor": "^2.5",
        "filament/filament": "^3.2.131",
        "filament/spatie-laravel-media-library-plugin": "^3.2.131",
        "filament/spatie-laravel-tags-plugin": "^3.2.131",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^10.48.25",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.29",
        "saade/filament-fullcalendar": "^3.2.2",
        "spatie/laravel-activitylog": "^4.9.1",
        "ysfkaya/filament-phone-input": "^2.3.7",
        "z3d0x/filament-logger": "^0.6.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.18.3",
        "laravel/sail": "^1.39.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^7.11",
        "phpunit/phpunit": "^10.5.40",
        "spatie/laravel-ignition": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
